# Ausgeschilderte, kombinierte Rad- und Fusswege:
# -----------------------------------------------
# Fuer den Radverkehr freigegebener Gehweg:
# - highway = footway, foot = designated, bicycle = yes
# - highway = path,    foot = designated, bicycle = yes
# - highway = path,    foot = official,   bicycle = yes
# Gemeinsamer Fuss- und Radweg:
# - highway = cycleway, foot = designated, bicycle = designated
# - highway = path,     foot = designated, bicycle = designated
# - highway = path,     foot = official,   bicycle = official
# Getrennter Fuss- und Radweg:
# - highway = cycleway, foot = designated, bicycle = designated, segregated = yes
# - highway = path,     foot = designated, bicycle = designated, segregated = yes
# - highway = path,     foot = official,   bicycle = official,   segregated = yes

highway = footway  & fahrrad = yes & fuss = yes                       {set highway = radfussweg}
highway = path     & fahrrad = yes & fuss = yes & bfdesignated = yes  {set highway = radfussweg}
# highway = cycleway & fahrrad = yes & fuss = yes                       {set highway = radfussweg}
highway = cycleway & fahrrad = yes & fuss = yes                       {set highway = radweg}


# Ausgeschilderter Fussweg (nur Fussgaenger erlaubt):
# ---------------------------------------------------
# - highway = path, foot = designated
# - highway = footway, foot = designated

highway = path    & fuss = yes & bfdesignated = yes {set highway = fussweg}
highway = footway & fuss = yes                      {set highway = fussweg}


# Ausgeschilderter Radweg (nur Radfahrer erlaubt):
# ------------------------------------------------
# - highway = cycleway, bicycle = designated
# - highway = path, bicycle = designated
# - highway = path, bicycle = official


highway = cycleway & fahrrad = yes                      {set highway = radweg}
highway = path     & fahrrad = yes & bfdesignated = yes {set highway = radweg}


# "radweg" verarbeiten:
# ---------------------

# Verbleibende Elemente "highway = cycleway" umdefinieren:
highway = cycleway {set highway = radweg}

# Bruecke / Tunnel / Deich
highway = radweg & bridge = * & bridge != no  [0x010407 resolution 22 continue]
highway = radweg & tunnel = yes               [0x010408 resolution 22 continue]
highway = radweg & man_made = dyke            [0x011f05 resolution 22 continue]
### highway = radweg & embankment = yes [0x011f05 resolution 22 continue]

# Allgemein oeffentlich
# 0x011f1d = Bitmap 2 (breit, NoLabel)
# 0x010f07 = Bitmap 2 (schmal, nur ein Pixel gefuellt, NoLabel)
highway = radweg {name "${name}" | "${ref}"} [0x011f1d resolution 24 continue]
highway = radweg [0x010f07 resolution 22-23]


# "fussweg" verarbeiten:
# ----------------------

# Verbleibende Elemente "highway=footway" umdefinieren:
highway = footway {set highway = fussweg}

# Bruecke / Tunnel / Deich
highway = fussweg & bridge = * & bridge != no  [0x010407 resolution 22 continue]
highway = fussweg & tunnel = yes               [0x010408 resolution 22 continue]
highway = fussweg & man_made = dyke            [0x011f05 resolution 22 continue]
### highway = fussweg & embankment = yes [0x011f05 resolution 24 continue]

# Benutzung verboten, kein Routing
# 0x010e11 = Bitmap 2 (breit, NoLabel)
# 0x010e12 = Bitmap 2 (schmal, nur ein Pixel gefuellt, NoLabel)
highway = fussweg & keinrouting = yes [0x010e11 resolution 24 continue]
highway = fussweg & keinrouting = yes [0x010e12 resolution 22-23]

# Allgemein oeffentlich
# 0x011f12 = 2 (breit, NoLabel)
# 0x010e05 = 1 (schmal, NoLabel)
highway = fussweg {set name = '${name}' | '${ref}'} [0x011f12 resolution 24 continue]
highway = fussweg [0x010e05 resolution 22-23]


