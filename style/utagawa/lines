highway = living_street & fzkrouting != yes { set fzkrouting=yes }[0x09 road_class=0 road_speed=0 resolution 24 continue with_actions]

# --------------------------------------------
# Andere mit dem Auto befahrbare Strasse
# --------------------------------------------
# 0x0a = Bitmap 2 (transparent, nur fuer Routing, Label)
auto = yes & fzkrouting != yes { set fzkrouting=yes } [0x0a road_class=1 road_speed=1 resolution 24 continue with_actions]

# --------------------------------------------
# make opposite cycleways, zweite Routingstrasse wird gezeichnet
# --------------------------------------------
# Achtung, es liegen zwei routbare Wege uebereinander, auf eventuelle Probleme bei der Indexsuche muss geachtet werden.
# 0x0e = Bitmap 2 (transparent, nur fuer Routing, NoLabel)
highway = * & (oneway = yes | oneway = 1 | oneway = true) & (cycleway = opposite | cycleway = opposite_lane | cycleway = opposite_track | oneway:bicycle = no | bicycle:oneway = no) {set oneway = ''; set motorcar = no; set bicycle = yes; set mkgmap:bicycle = yes} [0x0e road_speed=1 road_class=0 resolution 24 continue]
highway = * & oneway = -1 & (cycleway = opposite | cycleway=opposite_lane | cycleway = opposite_track | oneway:bicycle = no | bicycle:oneway = no) {set oneway = ''; set motorcar = no; set bicycle = yes; set mkgmap:bicycle = yes} [0x0e road_speed=1 road_class=0 resolution 24 continue]


# ==============================================
# ZEICHNEN VON Strassen und Wegen / highway:
# ==============================================

# Anmerkungen zu Strassennamen:
# resolution 21: keine Anzeige von Strassennamen
# resolution 22: Anzeige der Bezeichnungen / Namen von Autobahnen und autobahnaehnlichen Bundesstrassen (Schrift klein)
# resolution 23: wie resolution 22, zusaetzlich Anzeige der Namen von Bundesstrassen (Schrift klein)
# resolution 24: Anzeige aller Strassennamen (Schrift Default)

# Anmerkung zu Bruecke, Tunnel, Deich: Der Strassentyp ist irrelevant; entscheident ist die Strassenbreite.
# Gap   Bruecke      Tunnel     Deich
# ----  ----------  ----------  ----------
# 10    0x010409    0x01040a    0x011f02
# 8     0x01040b    0x01040c    0x011f03
# 6     0x010504    0x010406    0x011f04
# 4     0x010407    0x010408    0x011f05

# Autobahn:
# ---------
# Strasse mit baulich getrennten Fahrtrichtungen (i.A. Gruenstreifen) und besonderen Nutzungsbeschraenkungen.
# Typischerweise zwei Fahrspuren je Fahrtrichtung oder mehr und ein Standstreifen.

# Bruecke / Tunnel / Deich
highway = motorway & bridge = * & bridge != no   [0x010409 resolution 24 continue]
highway = motorway & tunnel = yes                [0x01040a resolution 24 continue]
highway = motorway & man_made = dyke             [0x011f02 resolution 24 continue]
### highway = motorway & embankment = yes [0x011f02 resolution 24 continue]

# Allgemein oeffentlich
# 0x011f13 = 5/1 (breit, NoLabel)
# 0x010f0a = 3/1 (mittel, Label, SmallFont)
# 0x010f0b = 1/1 (schmal, NoLabel)
# 0x010e0a = 1 (sehr schmal, NoLabel)
highway = motorway [0x011f13 resolution 24 continue]
highway = motorway [0x010f0a resolution 22-23 continue]
highway = motorway [0x010f0b resolution 20-21 continue]
#highway = motorway [0x010e0a resolution 17-19]
# starting at 14 for overviewmap
highway = motorway [0x010e0a resolution 14-19]


# Autobahnaehnliche Strasse:
# --------------------------
# Autobahnaehnlich ausgebaute Strasse, aber keine Autobahn. Typischerweise unter zentraler Verwaltung.

# Bruecke / Tunnel / Deich
highway = trunk & bridge = * & bridge != no     [0x01040b resolution 24 continue with_actions]
highway = trunk & tunnel = yes                  [0x01040c resolution 24 continue with_actions]
highway = trunk & man_made = dyke               [0x011f03 resolution 24 continue with_actions]
### highway = trunk & embankment = yes [0x011f03 resolution 24 continue with_actions]

# Allgemein oeffentlich
# 0x011f14 = 4/1 (breit, NoLabel)
# 0x012100 = 4/1 (breit, Label, SmallFont)
# 0x010f0c = 2/1 (mittel, NoLabel)
# 0x010f0d = 1/1 (schmal, NoLabel)
# 0x010e0b = 1 (sehr schmal, NoLabel)
# highway = trunk {set name = '${name}' | '${ref}' | ''} [0x011f14 resolution 24 continue with_actions]
# highway = trunk {set name = '${name}' | '${ref}' | ''} [0x012100 resolution 23-23 continue with_actions]
# highway = trunk {set name = '${ref}' | ''}             [0x010f0c resolution 22-22 continue with_actions]
# highway = trunk {set name = '${ref}' | ''}             [0x010f0d resolution 20-21 continue with_actions]
# highway = trunk {set name = '${ref}' | ''}             [0x010e0b resolution 18-19]
highway = trunk  [0x011f14 resolution 24 continue with_actions]
highway = trunk  [0x012100 resolution 23-23 continue with_actions]
highway = trunk  [0x010f0c resolution 22-22 continue with_actions]
highway = trunk  [0x010f0d resolution 20-21 continue with_actions]
highway = trunk  [0x010e0b resolution 18-19]

# Bundesstrasse:
# --------------
# Hauptverbindungsstrasse unter zentraler Verwaltung mit besonderer Kennzeichnung, die meist groessere Staedte
# verbindet und dem ueberregionalem Verkehr dient. Ausserdem: Strassen mit uebergeordneter Verkehrsbedeutung.

# Bruecke / Tunnel / Deich
highway = primary & bridge = * & bridge != no  [0x01040b resolution 24 continue with_actions]
highway = primary & tunnel = yes               [0x01040c resolution 24 continue with_actions]
highway = primary & man_made = dyke            [0x011f03 resolution 24 continue with_actions]
### highway = primary & embankment = yes       [0x011f03 resolution 24 continue with_actions]

# Allgemein oeffentlich
# 0x011f15 = 4/1 (breit, NoLabel)
# 0x012101 = 4/1 (breit, Label, SmallFont)
# 0x010f0e = 2/1 (mittel, NoLabel)
# 0x010f0f = 1/1 (schmal, NoLabel)
# 0x010e0b = 1 (sehr schmal, NoLabel)
# highway = primary {set name = '${name}' | '${ref}' | ''}        [0x011f15 resolution 24 continue with_actions]
# highway = primary {set name = '${name}' | '${ref}' | ''}        [0x012101 resolution 23-23 continue with_actions]
# highway = primary {set name = '${ref}' | ''}                    [0x010f0e resolution 22-22 continue with_actions]
# highway = primary {set name = '${ref}' | ''}                    [0x010f0f resolution 20-21 continue with_actions]
# highway = primary & motorroad = yes {set name = '${ref}' | ''}  [0x010e0b resolution 18-19]
highway = primary         [0x011f15 resolution 24 continue with_actions]
highway = primary         [0x012101 resolution 23-23 continue with_actions]
highway = primary         [0x010f0e resolution 22-22 continue with_actions]
highway = primary         [0x010f0f resolution 20-21 continue with_actions]
highway = primary & motorroad = yes   [0x010e0b resolution 18-19]


# Land- oder sehr gut ausgebaute Kreisstrasse:
# --------------------------------------------
# Strasse mit Mittellinie, die kleinere Staedte oder groessere Orte verbindet.
# Diese Strassen dienen dem zwischenregionalem Verkehr.

# Bruecke / Tunnel / Deich
highway = secondary & bridge = * & bridge != no [0x01040b resolution 24 continue with_actions]
highway = secondary & tunnel = yes              [0x01040c resolution 24 continue with_actions]
highway = secondary & man_made = dyke           [0x01040c resolution 24 continue with_actions]
### highway = secondary & embankment = yes [0x01040c resolution 24 continue with_actions]

# Allgemein oeffentlich
# 0x011f16 = 4/1 (breit, NoLabel)
# 0x010e0d = 4/1 (breit, NoLabel)
# 0x010f10 = 2/1 (mittel, NoLabel)
# 0x010f11 = 1/1 (schmal, NoLabel)
# highway = secondary {set name = '${ref}' | ''} [0x011f16 resolution 24 continue with_actions]
# highway = secondary {set name = '${ref}' | ''} [0x010e0d resolution 23-23 continue with_actions]
# highway = secondary {set name = '${ref}' | ''} [0x010f10 resolution 22-22 continue with_actions]
# highway = secondary {set name = '${ref}' | ''} [0x010f11 resolution 20-21]
highway = secondary  [0x011f16 resolution 24 continue with_actions]
highway = secondary  [0x010e0d resolution 23-23 continue with_actions]
highway = secondary  [0x010f10 resolution 22-22 continue with_actions]
highway = secondary  [0x010f11 resolution 20-21]


# Kreisstrasse:
# -------------
# Eine sehr gut ausgebaute Gemeindeverbindungsstrasse, die in erster Linie regionalen Verkehrsbeduerfnissen dient.
# Wenig befahrene Vorfahrtstrasse, die kleinere Orte verbindet. Innerstaedtische Vorfahrtstrassen mit Durchfahrtscharakter.

# Bruecke / Tunnel / Deich
highway = tertiary & bridge = * & bridge != no  [0x01040b resolution 24 continue with_actions]
highway = tertiary & tunnel = yes               [0x01040c resolution 24 continue with_actions]
highway = tertiary & man_made = dyke            [0x011f03 resolution 24 continue with_actions]
### highway = tertiary & embankment = yes [0x011f03 resolution 24 continue with_actions]

# Allgemein oeffentlich
# 0x010e0e = 4/1 (breit, NoLabel)
# 0x010f12 = 2/1 (mittel, NoLabel)
# 0x010f13 = 1/1 (schmal, NoLabel)
# highway = tertiary {set name = '${name}' | '${ref}' | ''} [0x010e0e resolution 24 continue with_actions]
# highway = tertiary {set name = '${ref}' | ''}             [0x010f12 resolution 22-23 continue with_actions]
# highway = tertiary {set name = '${ref}' | ''}             [0x010f13 resolution 20-21]
highway = tertiary  [0x010e0e resolution 24 continue with_actions]
highway = tertiary  [0x010f12 resolution 22-23 continue with_actions]
highway = tertiary  [0x010f13 resolution 20-21]


# Strasse an und in Wohngebieten:
# -------------------------------
# Die Strasse gehoert keiner anderen Strassenklasse an.

# Bruecke / Tunnel / Deich
highway = residential & (area != * | area = no) & bridge = * & bridge != no [0x01040b resolution 24 continue]
highway = residential & (area != * | area = no) & tunnel = yes              [0x01040c resolution 24 continue]
highway = residential & (area != * | area = no) & man_made = dyke           [0x011f03 resolution 24 continue]
### highway = residential & (area != * | area = no) & embankment = yes [0x011f03 resolution 24 continue]

# Benutzung verboten, kein Routing
# 0x010f16 = 4/1 (breit, NoLabel)
# 0x010f17 = 2/1 (mittel, NoLabel)
# 0x010105 = 1/1 (schmal, NoLabel)
highway = residential & (area != * | area = no) & keinrouting = yes [0x010f16 resolution 24 continue]
highway = residential & (area != * | area = no) & keinrouting = yes [0x010f17 resolution 23-23 continue]
highway = residential & (area != * | area = no) & keinrouting = yes [0x010105 resolution 21-22]

# Nicht allgemein oeffentlich (es duerfen keine Autos fahren)
# 0x010f16 = 4/1 (breit, NoLabel)
# 0x010f17 = 2/1 (mittel, NoLabel)
# 0x010105 = 1/1 (schmal, NoLabel)
highway = residential & (area != * | area = no) & auto = no [0x010f16 resolution 24 continue]
highway = residential & (area != * | area = no) & auto = no [0x010f17 resolution 23-23 continue]
highway = residential & (area != * | area = no) & auto = no [0x010105 resolution 21-22]

# Allgemein oeffentlich
# 0x011f17 = 4/1 (breit, NoLabel)
# 0x010f14 = 2/1 (mittel, NoLabel)
# 0x010f15 = 1/1 (schmal, NoLabel)
highway = residential & (area != * | area = no) [0x011f17 resolution 24 continue]
highway = residential & (area != * | area = no) [0x010f14 resolution 23-23 continue]
highway = residential & (area != * | area = no) [0x010f15 resolution 21-22]


# Oeffentlich befahrbare Nebenstrasse:
# ------------------------------------
# Eine Strasse mit einfachstem Ausbauzustand, und typischerweise ohne eine Mittellinie.
# Z.B. Gemeindestrassen mit Verbindungscharakter, die zu schmal sind, um als Kreisstrasse zu gelten.

# Bruecke / Tunnel / Deich
highway = unclassified & (area != * | area = no) & bridge = * & bridge != no  [0x01040b resolution 24 continue]
highway = unclassified & (area != * | area = no) & tunnel = yes               [0x01040c resolution 24 continue]
highway = unclassified & (area != * | area = no) & man_made = dyke            [0x011f03 resolution 24 continue]
### highway = unclassified & (area != * | area = no) & embankment = yes [0x011f03 resolution 24 continue]

# Benutzung verboten, kein Routing
# 0x010f1a = 3/1 (breit, NoLabel)
# 0x010f1b = 2/1 (mittel, NoLabel)
# 0x010f19 = 1/1 (schmal, NoLabel)
highway = unclassified & (area != * | area = no) & keinrouting = yes [0x010f1a resolution 24 continue]
highway = unclassified & (area != * | area = no) & keinrouting = yes [0x010f1b resolution 23-23 continue]
highway = unclassified & (area != * | area = no) & keinrouting = yes [0x010f19 resolution 21-22]

# Nicht allgemein oeffentlich (es duerfen keine Autos fahren)
# 0x010f1a = 3/1 (breit, NoLabel)
# 0x010f1b = 2/1 (mittel, NoLabel)
# 0x010f19 = 1/1 (schmal, NoLabel)
highway = unclassified & (area != * | area = no) & auto = no [0x010f1a resolution 24 continue]
highway = unclassified & (area != * | area = no) & auto = no [0x010f1b resolution 23-23 continue]
highway = unclassified & (area != * | area = no) & auto = no [0x010f19 resolution 21-22]

# Allgemein oeffentlich
# 0x011f18 = 3/1 (breit, NoLabel)
# 0x010f18 = 2/1 (mittel, NoLabel)
# 0x010106 = 1/1 (schmal, NoLabel)
highway = unclassified & (area != * | area = no) [0x011f18 resolution 24 continue]
highway = unclassified & (area != * | area = no) [0x010f18 resolution 23-23 continue]
highway = unclassified & (area != * | area = no) [0x010106 resolution 21-22]


# Erschliessungsweg zu Einrichtungen:
# -----------------------------------
# Erschliessungsweg z.B. zu Sportanlagen, Straenden, Autobahnraststaetten oder allgemein zu Gebaeuden.

# Bruecke / Tunnel / Deich
highway = service & (area != * | area = no) & bridge = * & bridge != no  [0x01040b resolution 24 continue]
highway = service & (area != * | area = no) & tunnel = yes               [0x01040c resolution 24 continue]
highway = service & (area != * | area = no) & man_made = dyke            [0x011f03 resolution 24 continue]
### highway = service & (area != * | area = no) & embankment = yes [0x011f03 resolution 24 continue]

# Benutzung verboten, kein Routing
# 0x010f1e = 3/1 (breit, NoLabel)
# 0x010f1f = 2/1 (mittel, NoLabel)
# 0x010f1d = 1/1 (schmal, NoLabel)
highway = service & (area != * | area = no) & keinrouting = yes [0x010f1e resolution 24 continue]
highway = service & (area != * | area = no) & keinrouting = yes [0x010f1f resolution 23-23 continue]
highway = service & (area != * | area = no) & keinrouting = yes [0x010f1d resolution 21-22]

# Nicht allgemein oeffentlich (es duerfen keine Autos fahren)
# 0x010f1e = 3/1 (breit, NoLabel)
# 0x010f1f = 2/1 (mittel, NoLabel)
# 0x010f1d = 1/1 (schmal, NoLabel)
highway = service & (area != * | area = no) & auto = no [0x010f1e resolution 24 continue]
highway = service & (area != * | area = no) & auto = no [0x010f1f resolution 23-23 continue]
highway = service & (area != * | area = no) & auto = no [0x010f1d resolution 21-22]

# Allgemein oeffentlich
# 0x011f19 = 3/1 (breit, NoLabel)
# 0x010f1c = 2/1 (mittel, NoLabel)
# 0x010107 = 1/1 (schmal, NoLabel)
highway = service & (area != * | area = no) [0x011f19 resolution 24 continue]
highway = service & (area != * | area = no) [0x010f1c resolution 23-23 continue]
highway = service & (area != * | area = no) [0x010107 resolution 21-22]


# Verkehrsberuhigter Bereich:
# ---------------------------
# Eine Strasse in der auf spielende Kinder aufgepasst werden muss, Fussgaenger
# bevorzugt behandelt werden und nur Schrittgeschwindigkeit erlaubt ist.

# Bruecke / Tunnel / Deich
highway = living_street & (area != * | area = no) & bridge = * & bridge != no  [0x01040b resolution 24 continue]
highway = living_street & (area != * | area = no) & tunnel = yes               [0x01040c resolution 24 continue]
highway = living_street & (area != * | area = no) & man_made = dyke            [0x011f03 resolution 24 continue]
### highway = living_street & (area != * | area = no) & embankment = yes [0x011f03 resolution 24 continue]

# Benutzung verboten, kein Routing
# 0x010f05 = 4/1 (breit, NoLabel)
# 0x010f06 = 2/1 (mittel, NoLabel)
# 0x010108 = 1/1 (schmal, NoLabel)
highway = living_street & (area != * | area = no) & keinrouting = yes [0x010f05 resolution 24 continue]
highway = living_street & (area != * | area = no) & keinrouting = yes [0x010f06 resolution 23-23 continue]
highway = living_street & (area != * | area = no) & keinrouting = yes [0x010108 resolution 21-22]

# Nicht allgemein oeffentlich (es duerfen keine Autos fahren)
# 0x010f05 = 4/1 (breit, NoLabel)
# 0x010f06 = 2/1 (mittel, NoLabel)
# 0x010108 = 1/1 (schmal, NoLabel)
highway = living_street & (area != * | area = no) & auto = no [0x010f05 resolution 24 continue]
highway = living_street & (area != * | area = no) & auto = no [0x010f06 resolution 23-23 continue]
highway = living_street & (area != * | area = no) & auto = no [0x010108 resolution 21-22]

# Allgemein oeffentlich
# 0x011f1a = 4/1 (breit, NoLabel)
# 0x010f03 = 2/1 (mittel, NoLabel)
# 0x010f04 = 1/1 (schmal, NoLabel)
highway = living_street & (area != * | area = no) [0x011f1a resolution 24 continue]
highway = living_street & (area != * | area = no) [0x010f03 resolution 23-23 continue]
highway = living_street & (area != * | area = no) [0x010f04 resolution 21-22]


#======================================================================================
# Beginn MTB / Fahrrad / Fuss / Wald / Berg / etc. Wege
#======================================================================================

# MTB naming Section
mbt:scale=* {add mtb:scale = '${mbt:scale}'}
mtb_scale=* {add mtb:scale = '${mtb_scale}'}

mtb:scale="0-1" {set mtb:scale="0+"}
mtb:scale="1-2" {set mtb:scale="1+"}
mtb:scale="2-3" {set mtb:scale="2+"}
mtb:scale="3-4" {set mtb:scale="3+"}
mtb:scale="4-5" {set mtb:scale="4+"}

mtb:scale="0/1" {set mtb:scale="0+"}
mtb:scale="1/2" {set mtb:scale="1+"}
mtb:scale="2/3" {set mtb:scale="2+"}
mtb:scale="3/4" {set mtb:scale="3+"}
mtb:scale="4/5" {set mtb:scale="4+"}

mtb:scale="0;1" {set mtb:scale="0+"}
mtb:scale="1;2" {set mtb:scale="1+"}
mtb:scale="2;3" {set mtb:scale="2+"}
mtb:scale="3;4" {set mtb:scale="3+"}
mtb:scale="4;5" {set mtb:scale="4+"}

mtb:scale:uphill="0-1" {set mtb:scale:uphill="0+"}
mtb:scale:uphill="1-2" {set mtb:scale:uphill="1+"}
mtb:scale:uphill="2-3" {set mtb:scale:uphill="2+"}
mtb:scale:uphill="3-4" {set mtb:scale:uphill="3+"}
mtb:scale:uphill="4-5" {set mtb:scale:uphill="4+"}

mtb:scale:uphill="0/1" {set mtb:scale:uphill="0+"}
mtb:scale:uphill="1/2" {set mtb:scale:uphill="1+"}
mtb:scale:uphill="2/3" {set mtb:scale:uphill="2+"}
mtb:scale:uphill="3/4" {set mtb:scale:uphill="3+"}
mtb:scale:uphill="4/5" {set mtb:scale:uphill="4+"}

mtb:scale:uphill="0;1" {set mtb:scale:uphill="0+"}
mtb:scale:uphill="1;2" {set mtb:scale:uphill="1+"}
mtb:scale:uphill="2;3" {set mtb:scale:uphill="2+"}
mtb:scale:uphill="3;4" {set mtb:scale:uphill="3+"}
mtb:scale:uphill="4;5" {set mtb:scale:uphill="4+"}


# make tracktype easier to read.
tracktype=grade0 {set tracktype=0}
tracktype=grade1 {set tracktype=1}
tracktype=grade2 {set tracktype=2}
tracktype=grade3 {set tracktype=3}
tracktype=grade4 {set tracktype=4}
tracktype=grade5 {set tracktype=5}
tracktype=grade6 {set tracktype=6}

tracktype="grade0;grade1" {set tracktype=0}
tracktype="grade1;grade2" {set tracktype=1}
tracktype="grade2;grade3" {set tracktype=2}
tracktype="grade3;grade4" {set tracktype=3}
tracktype="grade4;grade5" {set tracktype=4}
tracktype="grade5;grade6" {set tracktype=6}

tracktype="grade1;grade0" {set tracktype=0}
tracktype="grade2;grade1" {set tracktype=1}
tracktype="grade3;grade2" {set tracktype=2}
tracktype="grade4;grade3" {set tracktype=3}
tracktype="grade5;grade4" {set tracktype=4}
tracktype="grade6;grade5" {set tracktype=6}

tracktype=* & tracktype!=0 & tracktype!=1 & tracktype!=2 & tracktype!=3 & tracktype!=4 & tracktype!=5 & tracktype!=6 {delete tracktype}

highway=* & (usability=1 | usability=2) & mtb:scale:uphill!=* & mtb:scale!=*  {add tracktype=1}
highway=* & usability=3 				& mtb:scale:uphill!=* & mtb:scale!=*  {add taxi=no; add tracktype=2}
highway=* & usability=4 				& mtb:scale:uphill!=* & mtb:scale!=*  {add taxi=no; add tracktype=3; add mkgmap:unpaved=1}
highway=* & usability=5 				& mtb:scale:uphill!=* & mtb:scale!=*  {add taxi=no; add tracktype=4; add mkgmap:unpaved=1}
highway=* & usability=6 				& mtb:scale:uphill!=* & mtb:scale!=*  {add taxi=no;  set tracktype=5; add mkgmap:unpaved=1}

highway=* & trail_visibility=intermediate 	& mtb:scale!=* & mtb:scale:uphill!=* {add taxi=no; add tracktype=3; add mkgmap:unpaved=1}
highway=* & trail_visibility=bad 			& mtb:scale!=* & mtb:scale:uphill!=* {add taxi=no; add tracktype=4; add mkgmap:unpaved=1}
highway=* & trail_visibility=no 			& mtb:scale!=* & mtb:scale:uphill!=* {add taxi=no; add mkgmap:unpaved=1}
highway=* & trail_visibility=horrible 		& mtb:scale!=* & mtb:scale:uphill!=* {add taxi=no; add mkgmap:unpaved=1}
highway=* & trail_visibility=no 			& mtb:scale!=* & mtb:scale:uphill!=* & sac_scale!=* {add tracktype=5}
highway=* & trail_visibility=horrible 		& mtb:scale!=* & mtb:scale:uphill!=* & sac_scale!=* {add tracktype=5}

sac:scale=*  {add sac_scale = '${sac:scale}'}
sac_scale=T1 {set sac_scale=hiking}
sac_scale=T2 {set sac_scale=mountain_hiking}
sac_scale=T3 {set sac_scale=demandin_mountain_hiking}
sac_scale=T4 {set sac_scale=alpine_hiking}
sac_scale=T5 {set sac_scale=demanding_alpine_hiking}
sac_scale=T6 {set sac_scale=difficult_alpine_hiking}

sac_scale=* & sac_scale!=hiking & sac_scale!=mountain_hiking & sac_scale!=demanding_mountain_hiking & sac_scale!=alpine_hiking & sac_scale!=demanding_alpine_hiking & sac_scale!=difficult_alpine_hiking {delete sac_scale}

smoothnes=* {add smoothness = '${smoothnes}'}
smothness=* {add smoothness = '${smothness}'}

smoothness=excelent {set smoothness=excellent}
smoothness=medium {set smoothness=intermediate}
smoothness=* & smoothness!=excellent & smoothness!=very_good & smoothness!=good & smoothness!=intermediate & smoothness!=bad & smoothness!=very_bad & smoothness!=horrible & smoothness!=very_horrible & smoothness!=impassable {delete smoothness}

# Make steep pathes downhill only
mtb:scale=0 & incline>20 & mtb:scale:uphill!=1 & mtb:scale:uphill!=2 & mtb:scale:uphill!=3 {set oneway=-1; set copy=98; set copy1=yes}
mtb:scale=1 & incline>15 & mtb:scale:uphill!=1 & mtb:scale:uphill!=2 {set oneway=-1; set copy=98; set copy1=yes}
mtb:scale=2 & incline>10 & mtb:scale:uphill!=1 & mtb:scale:uphill!=2 {set oneway=-1; set copy=98; set copy1=yes}
mtb:scale=3 & incline>10 & mtb:scale:uphill!=1 & mtb:scale:uphill!=2 {set oneway=-1; set copy=98; set copy1=yes} 
mtb:scale=4 & incline>10 & mtb:scale:uphill!=1 & mtb:scale:uphill!=2 {set oneway=-1; set copy=98; set copy1=yes}
mtb:scale=5 & incline>10 & mtb:scale:uphill!=1 & mtb:scale:uphill!=2 {set oneway=-1; set copy=98; set copy1=yes}

mtb:scale=0 & incline<-20 & mtb:scale:uphill!=1 & mtb:scale:uphill!=2 & mtb:scale:uphill!=3	{set oneway=yes; set copy=99; set copy1=yes}
mtb:scale=1 & incline<-15 & mtb:scale:uphill!=1 & mtb:scale:uphill!=2 {set oneway=yes; set copy=99; set copy1=yes}
mtb:scale=2 & incline<-10 & mtb:scale:uphill!=1 & mtb:scale:uphill!=2 {set oneway=yes; set copy=99; set copy1=yes}
mtb:scale=3 & incline<-10 & mtb:scale:uphill!=1 & mtb:scale:uphill!=2 {set oneway=yes; set copy=99; set copy1=yes} 
mtb:scale=4 & incline<-10 & mtb:scale:uphill!=1 & mtb:scale:uphill!=2 {set oneway=yes; set copy=99; set copy1=yes}
mtb:scale=5 & incline<-10 & mtb:scale:uphill!=1 & mtb:scale:uphill!=2 {set oneway=yes; set copy=99; set copy1=yes}


( mtb:scale:uphill=2 | mtb:scale:uphill=3 ) & ( incline=negative | incline=down | incline=- | incline<0 ) 	{add taxi=no; set copy=99; set copy1=yes; add mkgmap:unpaved=1}
( mtb:scale:uphill=2 | mtb:scale:uphill=3 ) & ( incline=positive | incline=up | incline=+ | incline>0 ) 	{add taxi=no; set copy=98; set copy1=yes; add mkgmap:unpaved=1}
( mtb:scale:uphill=4 | mtb:scale:uphill=5 ) & ( incline=negative | incline=down | incline=- | incline<0 ) 	{add taxi=no; set oneway=yes; set copy=99; set copy1=yes; add mkgmap:unpaved=1}
( mtb:scale:uphill=4 | mtb:scale:uphill=5 ) & ( incline=positive | incline=up | incline=+ | incline>0 ) 	{add taxi=no; set oneway=-1; set copy=98; set copy1=yes; add mkgmap:unpaved=1}


# Map smoothness to tracktype
smoothness=excellent & tracktype=2 {delete smoothness; set tracktype=1}
smoothness=excellent & tracktype=3 {delete smoothness; set tracktype=1}
smoothness=excellent & tracktype=4 {delete smoothness; set tracktype=1; set taxi=no}
smoothness=excellent & tracktype=5 {delete smoothness; set tracktype=2}
smoothness=excellent {set tracktype=1}

smoothness=very_good & tracktype=2 {delete smoothness; set tracktype=1}
smoothness=very_good & tracktype=3 {delete smoothness; set tracktype=1; set taxi=no}
smoothness=very_good & tracktype=4 {delete smoothness; set tracktype=1; set taxi=no}
smoothness=very_good & tracktype=5 {delete smoothness; set tracktype=2}
smoothness=very_good {set tracktype=1}

smoothness=good & tracktype=2 {delete smoothness; set tracktype=1}
smoothness=good & tracktype=3 {delete smoothness; set tracktype=1; set taxi=no}
smoothness=good & tracktype=4 {delete smoothness; set tracktype=2}
smoothness=good & tracktype=5 {delete smoothness; set tracktype=2}
smoothness=good {set tracktype=1}

smoothness=intermediate & tracktype=2 {delete smoothness; set taxi=no}
smoothness=intermediate & tracktype=3 {delete smoothness; set tracktype=2}
smoothness=intermediate & tracktype=4 {delete smoothness; set tracktype=2}
smoothness=intermediate & tracktype=5 {delete smoothness; set tracktype=3}
smoothness=intermediate {set tracktype=1; set taxi=no}

smoothness=bad & tracktype=1 {delete smoothness; set tracktype=1; set taxi=no}
smoothness=bad & tracktype=2 {delete smoothness; set taxi=no}
smoothness=bad & tracktype=3 {delete smoothness; set tracktype=2; set taxi=no}
smoothness=bad & tracktype=4 {delete smoothness; set tracktype=2; set taxi=no; add mkgmap:unpaved=1}
smoothness=bad & tracktype=5 {delete smoothness; set tracktype=3; set taxi=no}
smoothness=bad {set tracktype=2; set taxi=no}

smoothness=very_bad & tracktype=1 {delete smoothness; set tracktype=2}
smoothness=very_bad & tracktype=2 {delete smoothness}
smoothness=very_bad & tracktype=3 {delete smoothness}
smoothness=very_bad & tracktype=4 {delete smoothness; set tracktype=3}
smoothness=very_bad & tracktype=5 {delete smoothness; set tracktype=3}
smoothness=very_bad {set tracktype=3}

smoothness=horrible & tracktype=1 {delete smoothness; set tracktype=2}
smoothness=horrible & tracktype=2 {delete smoothness; set tracktype=2; add mkgmap:unpaved=1}
smoothness=horrible & tracktype=3 {delete smoothness}
smoothness=horrible & tracktype=4 {delete smoothness; set tracktype=3}
smoothness=horrible & tracktype=5 {delete smoothness; set tracktype=4}
smoothness=horrible {set tracktype=3}

smoothness=very_horrible & tracktype=1 {delete smoothness; set tracktype=2; add mkgmap:unpaved=1}
smoothness=very_horrible & tracktype=2 {delete smoothness; set tracktype=3}
smoothness=very_horrible & tracktype=3 {delete smoothness}
smoothness=very_horrible & tracktype=4 {delete smoothness; set tracktype=3}
smoothness=very_horrible & tracktype=5 {delete smoothness; set tracktype=4}
smoothness=very_horrible {set tracktype=4}

