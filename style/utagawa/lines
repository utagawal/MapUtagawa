# highway = primary & fzkrouting != yes {set name = '${name}' | '${ref}' | ''; set fzkrouting=yes} [0x03 road_class=2 road_speed=5 resolution 24 continue with_actions]
highway = primary & fzkrouting != yes {set fzkrouting=yes} [0x03 road_class=2 road_speed=5 resolution 24 continue with_actions]

# --------------------------------------------
# secondary
