# 0x011f11 = Bitmap 4 (NoLabel)
highway = steps [0x011f11 resolution 22 continue]


# Weg oder Strasse nur fuer Fussgaenger:
# --------------------------------------
# Z.B. Fussgaengerstrasse, -zone, typisch fuer Einkaufspassagen.

# Bruecke / Tunnel / Deich
highway = pedestrian & (area != * | area = no) & bridge = * & bridge != no  [0x01040b resolution 22 continue]
highway = pedestrian & (area != * | area = no) & tunnel = yes               [0x01040c resolution 22 continue]
highway = pedestrian & (area != * | area = no) & man_made = dyke            [0x011f03 resolution 22 continue]
### highway = pedestrian & (area != * | area = no) & embankment = yes [0x011f03 resolution 22 continue]

# Allgemein oeffentlich
# 0x011f1b = 3/1 (breit, NoLabel)
# 0x010f08 = 2/1 (mittel, NoLabel)
# 0x010f09 = 1/1 (schmal, NoLabel)
highway = pedestrian & (area != * | area = no) [0x011f1b resolution 24 continue]
highway = pedestrian & (area != * | area = no) [0x010f08 resolution 22-23 continue]
highway = pedestrian & (area != * | area = no) [0x010f09 resolution 20-22]


# Bergwanderweg, typischerweise im Gebirge:
# -----------------------------------------
# - hat sac_scale-Attribut:
#   - sac_scale=hiking = T1 (Wandern)
#   - sac_scale=mountain_hiking = T2 (Bergwandern)
#   - sac_scale=demanding_mountain_hiking = T3 (erfahrener Bergwanderer )
#   - sac_scale=alpine_hiking = T4 (Alpinwandern )
#   - sac_scale=demanding_alpine_hiking = T5 (anspruchsvolles Alpinwandern )
#   - sac_scale=difficult_alpine_hiking = T6 (schwieriges Alpinwandern)
# Routing nur auf T1-T3 (aus Sicherheitsgruenden).

# Bruecke / Tunnel / Deich
#(highway = bergweg12 | highway = bergweg34 | highway = bergweg56) & bridge = yes               [0x010407 resolution 24 continue]
#(highway = bergweg12 | highway = bergweg34 | highway = bergweg56) & tunnel = yes               [0x010408 resolution 24 continue]
#(highway = bergweg12 | highway = bergweg34 | highway = bergweg56) & man_made = dyke            [0x011f05 resolution 24 continue]
### (highway = bergweg12 | highway = bergweg34 | highway = bergweg56) & embankment = yes [0x011f05 resolution 24 continue]
(highway = sacscalet1 | highway = sacscalet2t3 | highway = sacscalet4t5t6) & bridge = * & bridge != no  [0x010407 resolution 22 continue]
(highway = sacscalet1 | highway = sacscalet2t3 | highway = sacscalet4t5t6) & tunnel = yes               [0x010408 resolution 22 continue]
(highway = sacscalet1 | highway = sacscalet2t3 | highway = sacscalet4t5t6) & man_made = dyke            [0x011f05 resolution 22 continue]

# 0x010e0c = Bitmap 2 (breit, NoLabel)
# 0x010e06 = Bitmap 2 (schmal, nur ein Pixel gefuellt, NoLabel)
highway = sacscalet1 {set name = '${name}' | '${ref}'} [0x010e0c resolution 24 continue]
highway = sacscalet1 [0x010e06 resolution 22-23]

# 0x011f09 = Bitmap 2 (breit, NoLabel)
# 0x011f0c = Bitmap 2 (schmal, nur ein Pixel gefuellt, NoLabel)
#highway = bergweg12 {set name = '${name}' | '${ref}'} [0x011f09 resolution 24 continue]
#highway = bergweg12 [0x011f0c resolution 22-23]

# 0x011f07 = Bitmap 2 (breit, NoLabel)
# 0x011f0a = Bitmap 2 (schmal, nur ein Pixel gefuellt, NoLabel)
#highway = bergweg34 [0x011f07 resolution 24 continue]
#highway = bergweg34 [0x011f0a resolution 22-23]
highway = sacscalet2t3 {set name = '${name}' | '${ref}'} [0x011f07 resolution 24 continue]
highway = sacscalet2t3 [0x011f0a resolution 22-23]

# 0x011f08 = Bitmap 2 (breit, NoLabel)
# 0x011f0b = Bitmap 2 (schmal, nur ein Pixel gefuellt, NoLabel)
#highway = bergweg56 [0x011f08 resolution 24 continue]
#highway = bergweg56 [0x011f0b resolution 23-23]
highway = sacscalet4t5t6 {set name = '${name}' | '${ref}'} [0x011f08 resolution 24 continue]
highway = sacscalet4t5t6 [0x011f0b resolution 22-23]


# Allgemeiner Weg oder Pfad:
# --------------------------
# - keine Nutzungsart vorgegeben
# - nicht fuer motorisierte Fahrzeuge gedacht
# - fuer Fussgaenger, Radfahrer und Reiter

# Bruecke / Tunnel / Deich
highway = path & bridge = * & bridge != no  [0x010407 resolution 22 continue]
highway = path & tunnel = yes               [0x010408 resolution 22 continue]
highway = path & man_made = dyke            [0x011f05 resolution 22 continue]
### highway = path & embankment = yes [0x011f05 resolution 24 continue]

# Benutzung verboten, kein Routing
# 0x010e11 = Bitmap 2 (breit, NoLabel)
# 0x010e12 = Bitmap 2 (schmal, nur ein Pixel gefuellt, NoLabel)
highway = path & keinrouting = yes [0x010e11 resolution 24 continue]
highway = path & keinrouting = yes [0x010e12 resolution 22-23]

# Allgemein oeffentlich
# 0x010e0c = Bitmap 2 (breit, NoLabel)
# 0x010e06 = Bitmap 2 (schmal, nur ein Pixel gefuellt, NoLabel)
highway = path  & (area != * | area = no) [0x010e0c resolution 24 continue]
highway = path  & (area != * | area = no) [0x010e06 resolution 22-23]


# Wirtschafts-, Feld- oder Waldweg:
# ---------------------------------
#
# - sollte mit tracktype = 1-5 naeher spezifiziert werden
# - der Weg ist breit genug fuer ein vierraedriges Fahrzeug
#
# Fuer die Mehrheit der Feld- / Waldwege gilt:
# - Die Feldwege sind nicht fuer den oeffentlichen Kraftverkehr freigegeben.
# - Die Feldwege koennen durch Fussgaenger und Radfahrer benutzt werden.
# - Diese Feldwege werden in der Farbe "braun" dargestellt.
#
# Fuer einige Feld- / Waldwege gilt:
# - Die Feldwege sind "privat" und duerfen nicht fuer das Routing benutzt werden.
# - Diese Feldwege werden in der Farbe "lila" dargestellt.
#
# Fuer einige Feld- / Waldwege gilt:
# - Diese Feldwege sind explizit fuer den oeffentlichen Kraftverkehr freigegeben.
# - Damit ist dies kein Feldweg mehr, sondern eine "Strasse unbekannter Klassifikation".
# - Die Darstellung erfolgt entsprechend oder aehnlich wie "highway = unclassified".

highway = track & tracktype = 1 {set highway = feldweg1}
highway = track & tracktype = 2 {set highway = feldweg023}
highway = track & tracktype = 3 {set highway = feldweg023}
highway = track & tracktype = 4 {set highway = feldweg45}
highway = track & tracktype = 5 {set highway = feldweg45}

# Verbleibende Elemente "highway=track" umdefinieren (0 = ohne tracktype-Angabe):
highway = track {set highway = feldweg023}


# Feld- / Waldweg der Gueteklasse 1:
# ----------------------------------

# Bruecke / Tunnel / Deich
highway = feldweg1 & bridge = * & bridge != no  [0x010405 resolution 22 continue]
highway = feldweg1 & tunnel = yes               [0x010406 resolution 22 continue]
highway = feldweg1 & man_made = dyke            [0x011f04 resolution 22 continue]
# highway = feldweg1 & enbankment = yes [0x011f04 resolution 24 continue]

# Benutzung verboten, kein Routing
# 0x010e13 = 3 (breit, NoLabel)
# 0x010e14 = 2 (schmal, NoLabel)
highway = feldweg1 & keinrouting = yes [0x010e13 resolution 24 continue]
highway = feldweg1 & keinrouting = yes [0x010e14 resolution 22-23]

# Nicht fuer Autos freigegeben oder öffentlich
# 0x010e09 = 3 (breit, NoLabel)
# 0x010e07 = 2 (schmal, NoLabel)
highway = feldweg1  [0x010e09 resolution 24 continue]
highway = feldweg1  [0x010e07 resolution 22-23]

# Replaced with above, no distinction for auto=yes or auto=no
## Nicht fuer Autos freigegeben
## 0x010e09 = 3 (breit, NoLabel)
## 0x010e07 = 2 (schmal, NoLabel)
#highway = feldweg1 & auto = no [0x010e09 resolution 24 continue]
#highway = feldweg1 & auto = no [0x010e07 resolution 23-23]
#
## Allgemein oeffentlich
## 0x010f18 = 2/1 (mittel, NoLabel) (wie highway=unclassified)
## 0x010106 = 1/1 (schmal, NoLabel) (wie highway=unclassified)
#highway = feldweg1 [0x010f18 resolution 24 continue]
#highway = feldweg1 [0x010106 resolution 23-23]


